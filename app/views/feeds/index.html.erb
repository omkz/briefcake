  


<div class="mt-16 max-w-xl mx-auto">
  <header class="flex justify-between">
    <h1 class="text-3xl mb-4">Feeds</h1>

    <div>
      <%= link_to "Add", new_feed_path, class: "btn btn--primary" %>
    </div>
  </header>

  <table class="table">
    <thead>
      <tr>
    

            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @feeds.each do |feed| %>
        <%= content_tag :tr, id: dom_id(feed) do %>
          

          
          
            <td><%= link_to "Show", feed %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>

